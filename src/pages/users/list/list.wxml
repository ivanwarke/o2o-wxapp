<view >
  <view class="weui-cells weui-cells_after-title">
    <navigator url="/pages/users/item/item?type=text&label=姓名&val={{fullName}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">姓名</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">{{fullName}}</view>
    </navigator>
    <navigator url="/pages/users/item/item?type=text&label=手机号&val={{phone}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">手机号</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">{{phone}}</view>
    </navigator>
    <navigator url="/pages/users/item/item?type=text&label=密码&val=" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">密码</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">已设置</view>
    </navigator>
    <navigator url="/pages/users/item/item?type=text&label=邮箱&val={{email}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">邮箱</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">{{email}}</view>
    </navigator>
    <navigator url="/pages/users/item/item?type=text&label=收货地址&val={{address}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">收货地址</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">{{ addressShort }}</view>
    </navigator>
    <navigator url="/pages/users/item/item?type=date&label=生日&val={{birthday}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">生日</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">{{birthday}}</view>
    </navigator>
  </view>
</view>